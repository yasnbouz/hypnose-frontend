{
  "name": "hypnose-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test:jest": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "test:cy": "start-server-and-test dev 3000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "framer-motion": "4",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "babel-jest": "^27.0.6",
    "cypress": "^7.6.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.12.5",
    "typescript": "4.3.5"
  }
}
